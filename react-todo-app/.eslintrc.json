{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        //"eslint:recommended",
        "plugin:react/recommended"
    ],
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "react","jsx-a11y", "import"
    ],
    "rules": {
        "quotes": ["error", "double", { "avoidEscape": true }],
        "indent": ["error", 2],
        "react/jsx-filename-extension": 0,
        "react/prop-types": "off"
    }
}
